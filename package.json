{
	"name": "helloworld",
	"publisher": "shinjini-ghosh",
	"displayName": "HelpMe",
	"description": "Access frequently used helper code links without leaving VS Code",
	"version": "0.0.6",
	"icon": "images/logo.png",
	"galleryBanner": {
		"color": "#d3d3d3",
		"theme": "light"
	},
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:helloworld.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "helloworld.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^10.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"jsonfile": "^6.0.1",
		"path": "^0.12.7"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/shinjinighosh/helpme-vscode-extension"
	},
	"license": "See license in LICENSE",
	"bugs": {
		"url": "https://github.com/shinjinighosh/helpme-vscode-extension/issues",
		"email": "shinghos@mit.edu"
	},
	"homepage": "https://github.com/shinjinighosh/helpme-vscode-extension/blob/master/README.md"


}
